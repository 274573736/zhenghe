page {
    background: #ffffff;
}

.coupon-left {
    background: url("data:image/png;base64,") no-repeat center;
    min-width: 294rpx;
    height: 134rpx;
    font-size: 24rpx;
    color: #ffffff;
    background-size: 100% 100%;
}

.coupon-title {
    width: 26rpx;
    color: #95c5f7;
    font-size: 20rpx;
    margin-left: 20rpx;
}

.coupon-des {
    font-size: 20rpx;
    margin-left: 50rpx;
}

.coupon-money {
    font-size: 60rpx;
    font-weight: bold;
}

.rows {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.top {
    width: 750rpx;
    height: 250rpx;
}

.top-box {
    padding: 30rpx 40rpx;
}

.coupon-name {
    font-weight: bold;
}

.coupon-price {
    color: #097bf1;
    font-size: 24rpx;
}

.center-box {
    width: 100%;
    padding: 30rpx 40rpx;
    border-top: 1px solid #e1e1e1;
    border-bottom: 1px solid #e1e1e1;
}

.rule {
    margin-top: 30rpx;
}

.goods_des {
    color: #808080;
    font-size: 24rpx;
    margin-top: 10rpx;
}

.footer {
    width: 100%;
    padding: 0 40rpx;
    position: fixed;
    bottom: 40rpx;
}

.footer-btn {
    width: 670rpx;
    height: 80rpx;
    color: #ffffff;
    font-weight: bold;
    background: #097bf1;
    border-radius: 35px;
}