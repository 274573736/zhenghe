page {
    background: #f6f6f6;
}

.top {
    width: 100%;
    height: 100rpx;
    font-size: 36rpx;
    font-weight: bold;
    background: #ffffff;
    padding-left: 40rpx;
    padding-top: 20rpx;
}

.info-box {
    width: 100%;
    height: 320rpx;
    background: #ffffff;
    border-bottom: 1px solid #e9e9e9;
}

.info {
    width: 750rpx;
    height: 300rpx;
    color: #ffffff;
    background: url("data:image/png;base64,") no-repeat center;
    background-size: 710rpx 300rpx;
    padding: 40rpx 0 40rpx 0;
    justify-content: space-between;
}

.title {
    font-size: 26rpx;
    color: #afd7ff;
}

.head-position {
    margin-left: 80rpx;
    margin-right: 20rpx;
}

.head-img {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    overflow: hidden;
}

.service-form {
    margin-right: 60rpx;
}

.service-num {
    padding: 0 20rpx;
    color: #ffffff;
}

.service-num-animation {
    color: #097bf1;
    opacity: 0;
}

.service-btn {
    border: 1px solid #ffffff;
    border-radius: 5px;
    padding: 5rpx 10rpx;
    box-sizing: border-box;
    background: #2f91f5;
    color: #ffffff;
    font-size: 26rpx;
}

.service-btn-active {
    background: #097bf1;
}

.ele-box {
    font-size: 24rpx;
    width: 260rpx;
    height: 40rpx;
    text-align: center;
    border: 1px solid #ffffff;
    border-radius: 5px;
    margin-left: 20rpx;
}

.money {
    font-size: 44rpx;
    font-weight: bold;
}

.big-staff-box {
    width: 750rpx;
    height: 200rpx;
    background: #ffffff;
    padding: 0 40rpx;
    font-size: 26rpx;
    font-weight: bold;
    margin-top: 20rpx;
    border-top: 1px solid #e9e9e9;
}

.big-staff-box>view {
    width: 50%;
    height: 100%;
}

.info-footer {
    padding: 0 90rpx;
}

.staff-img {
    width: 54rpx;
    height: 54rpx;
    margin-bottom: 20rpx;
}

.order-img {
    width: 50rpx;
    height: 54rpx;
    margin-bottom: 20rpx;
}

.address {
    width: 440rpx;
    text-align: right;
}

.item {
    width: 100%;
    height: 100rpx;
    font-size: 26rpx;
    padding: 0 40rpx;
    background: #ffffff;
    border-top: 1px solid #dfdfdf;
}

.btn2 {
    width: 320rpx;
    height: 320rpx;
    border-radius: 50%;
    background: linear-gradient(to bottom,#9dcdff,#ffffff);
    animation: suo 2s linear infinite;
}

.btn1 {
    width: 250rpx;
    height: 250rpx;
    border-radius: 50%;
    background: linear-gradient(to bottom,#097bf1,#e0ecf8);
}

.btn {
    font-size: 36rpx;
    width: 170rpx;
    height: 170rpx;
    border-radius: 50%;
    color: #ffffff;
    background: #097bf1;
    box-shadow: 0 0 10px #045bea inset;
}

.footer {
    flex: 1;
}

@keyframes suo {
    0% {
        transform: scale(1);
    }

    10% {
        transform: scale(0.95);
    }

    20% {
        transform: scale(0.9);
    }

    30% {
        transform: scale(0.85);
    }

    40% {
        transform: scale(0.8);
    }

    50% {
        transform: scale(0.75);
    }

    60% {
        transform: scale(0.8);
    }

    70% {
        transform: scale(0.85);
    }

    80% {
        transform: scale(0.9);
    }

    90% {
        transform: scale(0.95);
    }

    100% {
        transform: scale(1);
    }
}

.wrap__uc-waves {
    overflow: hidden;
    height: 1rem;
    width: 100%;
    position: absolute;
    bottom: 0;
}

.wrap__uc-waves .wave {
    width: 15rem;
    transform-origin: center bottom;
    position: absolute;
    left: 0;
    bottom: 0;
}

.wrap__uc-waves .w1 {
    background: url(../images/icon__uc-hd-waves01.png-do-not-use-local-path-./make_speed/big_customer/info/info.wxss&210&32) no-repeat;
    background-size: cover;
    height: .5rem;
    animation: anim_wave 5s linear infinite;
}

.wrap__uc-waves .w2 {
    background: url(../images/icon__uc-hd-waves02.png-do-not-use-local-path-./make_speed/big_customer/info/info.wxss&211&32) no-repeat;
    background-size: cover;
    height: .7rem;
    animation: anim_wave 6s linear infinite;
}

@keyframes anim_wave {
    0% {
        transform: translateX(0) translateZ(0) scaleY(1);
    }

    50% {
        transform: translateX(-25%) translateZ(0) scaleY(0.55);
    }

    100% {
        transform: translateX(-50%) translateZ(0) scaleY(1);
    }
}